<% if @status == 0 %>
  $("#waiting").hide();
  tishi("编辑失败!");
<% else %>
  $("#waiting").hide();
  $(".second_bg").hide();
  $("#set_functions_edit").hide();
  tishi("编辑成功!");
  $.ajax({
    type: "get",
    url: "/stores/"+<%= @store.id %>+"/set_functions",
    dataType: "script",
    data: {
    type : <%= @type %>
  }
  })
<% end %>